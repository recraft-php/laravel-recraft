{
    "name": "recraft-php/laravel-recraft",
    "description": "A Laravel SDK for Recraft AI",
    "type": "library",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.9",
        "laravel/framework": "^10.0"
    },
    "require-dev": {
        "pestphp/pest": "^2.0",
        "orchestra/testbench": "^8.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Recraft\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Julius George",
            "email": "julius.business12@gmail.com",
            "homepage": "https://facebook.com/shiningge.julius"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Recraft\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Recraft\\RecraftServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
